#include<bits/stdc++.h>
using namespace std;
string s="\0";
int n=1000;
int main() {
    while(n--) {
    	getline(cin,s); 
        for(int i=0; i<s.size(); i++) {
            if(s[i] >= 97 && s[i] <= 122) {
                cout << (char)(s[i]-32);
            }
            else cout << s[i]; 
        }
        cout<<"\n";
    }
}
